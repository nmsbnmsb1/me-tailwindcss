{
  "name": "me-tailwindcss",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "bin": {
    "inspect-tw-config": "./lib/cmd-inspect-tw-config.js",
    "build-css": "./lib/cmd-build-css.js"
  },
  "scripts": {
    "tw": "node ./lib/cmd-inspect-tw-config.js --input ./examples/tw-config.js --output ./examples/tw-config.json",
    "tw2": "node ./lib/cmd-inspect-tw-config.js --input ./examples/tw-config2.js --output ./examples/tw-config2.json",
    "css": "node ./lib/cmd-build-css.js --input ./examples/index.css --tw ./examples/tw-config.js --output ./examples/theme.css",
    "css2": "node ./lib/cmd-build-css.js --input ./examples/index.css --tw ./examples/tw-config2.js --output ./examples/theme2.css"
  },
  "dependencies": {
    "tailwindcss": "^1.3.1",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.3",
    "postcss-extend-rule": "^3.0.0",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-property-lookup": "^2.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-color-mod-function": "^3.0.3",
    "cssnano": "^4.1.10"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prettier-tslint": "^0.4.2"
  }
}
