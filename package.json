{
  "name": "me-tailwindcss",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "bin": {
    "inspect-tw-config": "./lib/cmd-inspect-tw-config.js",
    "build-css": "./lib/cmd-build-css.js"
  },
  "scripts": {
    "tw": "node ./lib/cmd-inspect-tw-config.js --input ./styles/tw.config.js --output ./tw.config.json",
    "tw2": "node ./lib/cmd-inspect-tw-config.js --input ./styles/tw.config2.js --output ./tw.config2.json",
    "css": "node ./lib/cmd-build-css.js --input ./styles/index.css --tw ./styles/tw.config.js --output ./theme.css",
    "css2": "node ./lib/cmd-build-css.js --input ./styles/index.css --tw ./styles/tw.config2.js --output ./theme2.css",
    "css-post": "node ./lib/cmd-build-css.js --input ./styles/index.css --postcss ./postcss.config.js --output ./theme-post.css"
  },
  "dependencies": {
    "tailwindcss": "^1.3.5",
    "postcss": "^7.0.27",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.3",
    "postcss-extend-rule": "^3.0.0",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-property-lookup": "^2.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-color-mod-function": "^3.0.3",
    "cssnano": "^4.1.10"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prettier-tslint": "^0.4.2",
    "purgecss": "^2.1.2",
    "postcss-purgecss": "^2.0.3"
  }
}
